{
  "manifest_version": 2,
  "name": "Google Scholar Citation Count",
  "version": "4.0.0",
  "description": "Zotero plugin for fetching numbers of citations from Google Scholar.",
  "homepage_url": "https://github.com/justinribeiro/zotero-google-scholar-citation-count",
  "author": "Justin Ribeiro",
  "icons": {
    "48": "chrome/content/icons/favicon.png",
    "96": "chrome/content/icons/faviconX2.png"
  },
  "applications": {
    "zotero": {
      "id": "justin@justinribeiro.com",
      "update_url": "https://github.com/justinribeiro/zotero-google-scholar-citation-count/updates.json",
      "strict_min_version": "6.999",
      "strict_max_version": "7.0.*"
    }
  }
}
